<?xml version="1.0" encoding="UTF-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="horizontal" >
    <TextView
        android:id="@+id/search_index_text"
        android:layout_width="0dp"
        android:layout_height="fill_parent"
        android:layout_weight="78" 
        android:textColor="#ffc8c8c8"
        android:text="1"
        android:textSize="15sp"
        android:textStyle="bold"
        android:gravity="center"
        />
    <FrameLayout 
        android:layout_weight="150"
        android:layout_width="0dp"
    	android:layout_height="fill_parent">
        <LinearLayout 
            android:layout_width="fill_parent"
    		android:layout_height="fill_parent"
    		android:orientation="vertical">
    		<View
                android:layout_width="fill_parent"
    			android:layout_height="0dp"
    			android:layout_weight="30" />
            <LinearLayout 
	            android:layout_width="fill_parent"
	    		android:layout_height="0dp"
	    		android:layout_weight="130" 
	    		android:orientation="horizontal">
	    		<ImageView
	    		    android:id="@+id/list_img"
	    		    android:layout_width="0dp"
    				android:layout_height="fill_parent"
    				android:layout_weight="130"
    				android:background="@drawable/list_defimg"
	    		    />
	    		<View 
	    		    android:layout_width="0dp"
    				android:layout_height="fill_parent"
    				android:layout_weight="20"/>
    		</LinearLayout>
    		<View
                android:layout_width="fill_parent"
    			android:layout_height="0dp"
    			android:layout_weight="20" />
        </LinearLayout>
        <LinearLayout 
            android:layout_width="fill_parent"
    		android:layout_height="fill_parent"
    		android:orientation="vertical">
    		<View
                android:layout_width="fill_parent"
    			android:layout_height="0dp"
    			android:layout_weight="16" />
            <LinearLayout 
	            android:layout_width="fill_parent"
	    		android:layout_height="0dp"
	    		android:layout_weight="48" 
	    		android:orientation="horizontal">
	    		<View 
	    		    android:layout_width="0dp"
    				android:layout_height="fill_parent"
    				android:layout_weight="100"/>
	    		<TextView
	    		    android:id="@+id/list_notify"
	    		    android:layout_width="0dp"
    				android:layout_height="fill_parent"
    				android:layout_weight="49"
    				android:gravity="center"
    				android:text="S"
    				android:textColor="#ffffffff"
    				android:visibility="visible"
    				android:background="@drawable/list_s_img"/>
	    		<View 
	    		    android:layout_width="0dp"
    				android:layout_height="fill_parent"
    				android:layout_weight="1"/>
    		</LinearLayout>
    		<View
                android:layout_width="fill_parent"
    			android:layout_height="0dp"
    			android:layout_weight="116" />
        </LinearLayout>
    </FrameLayout>
    <View
        android:layout_weight="10"
        android:layout_width="0dp"
    	android:layout_height="fill_parent" />
    <LinearLayout
        android:layout_weight="814"
        android:layout_width="0dp"
    	android:layout_height="fill_parent" 
    	android:orientation="vertical">
        <View 
            android:layout_weight="10"
        	android:layout_width="fill_parent"
    		android:layout_height="0dp" />
         <TextView
            android:id="@+id/list_subject"
            android:layout_weight="30"
     		android:gravity="left|center_vertical"
 			android:layout_width="fill_parent"
 			android:layout_height="0dp"
 			android:maxLines="1"
 			android:textSize="14sp"
 			android:singleLine="true"
 			android:textColor="#5d5d5d"
 			android:text="testasdasdasdasdasdasdasdasdasdasdasd"
             />
          <TextView
            android:id="@+id/list_nickname" 
            android:layout_weight="20"
     		android:gravity="left|top"
 			android:layout_width="fill_parent"
 			android:layout_height="0dp"
 			android:maxLines="1"
 			android:singleLine="true"
 			android:textColor="#cb3333"
 			android:textSize="10sp"
 			android:text="testasdasdasdasdasdasdasdasdasdasdasd"
             />
           <TextView
            android:id="@+id/list_inapp" 
            android:layout_weight="20"
     		android:gravity="left|top"
 			android:layout_width="fill_parent"
 			android:layout_height="0dp"
 			android:maxLines="1"
 			android:singleLine="true"
 			android:textColor="#C7C7C7"
 			android:textSize="10sp"
 			android:text="In-App HAD"
             />
        <View 
            android:layout_weight="5"
        	android:layout_width="fill_parent"
    		android:layout_height="0dp" />
        <LinearLayout
            android:layout_weight="35"
        	android:layout_width="fill_parent"
    		android:layout_height="0dp"
    		android:orientation="horizontal"  >
            <TextView
                android:id="@+id/list_totalscore"
                android:layout_weight="3"
        		android:layout_width="0dp"
        		android:gravity="left|center_vertical"
    			android:layout_height="fill_parent" 
    			android:textColor="#5d5d5d"
    			android:textSize="10sp"
    			android:text="총점수:33232"
                />
            <TextView
                android:id="@+id/list_point"
                android:layout_weight="1"
        		android:layout_width="0dp"
        		android:gravity="right|center_vertical"
    			android:layout_height="fill_parent" 
    			android:textColor="#5d5d5d"
    			android:textSize="10sp"
    			android:text="100p"
                />
        </LinearLayout>
    </LinearLayout>
    <View
        android:layout_width="0dp"
        android:layout_height="fill_parent"
        android:layout_weight="33"
        />
    <LinearLayout
        android:layout_width="0dp"
        android:layout_height="fill_parent"
        android:layout_weight="21"
        android:orientation="vertical"
         >
        <View
	        android:layout_width="fill_parent"
	        android:layout_height="0dp"
	        android:layout_weight="58"
        />
        <View
	        android:layout_width="fill_parent"
	        android:layout_height="0dp"
	        android:layout_weight="33"
	        android:background="@drawable/list_arrow"
	        />
        <View
	        android:layout_width="fill_parent"
	        android:layout_height="0dp"
	        android:layout_weight="59"
	        />
    </LinearLayout>
    <View
        android:layout_width="0dp"
        android:layout_height="fill_parent"
        android:layout_weight="32"
        />
 </LinearLayout>